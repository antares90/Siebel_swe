<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <security>
            <requestFiltering>
                <fileExtensions>
                    <add fileExtension=".data" allowed="true" />
                    <add fileExtension=".mem" allowed="true" />
                    <add fileExtension=".bc" allowed="true" />
                </fileExtensions>
            </requestFiltering>
        </security>
        <directoryBrowse enabled="true" />
    </system.webServer>
</configuration>
